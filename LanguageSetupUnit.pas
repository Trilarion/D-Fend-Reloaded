destructor TLanguageSetup.Destroy;
begin
  StoreAllValues;
  inherited Destroy;
end;

Function RemoveUnderline(const S : String) : String;
Var I : Integer;
begin
  result:='';
  for I:=1 to length(S) do if S[I]<>'&' then result:=result+S[I];
end;

Procedure LoadLanguage(const LanguageFile : String);
begin
  FreeAndNil(LanguageSetup);
  If FileExists(PrgDir+LanguageSubDir+'\'+LanguageFile)
    then LanguageSetup:=TLanguageSetup.Create(LanguageFile)
    else LanguageSetup:=TLanguageSetup.Create;
end;

initialization
  LanguageSetup:=nil;
  LoadLanguage(PrgSetup.Language);
finalization
  LanguageSetup.Free;
end.
