<?xml version="1.0" encoding="ISO-8859-1"?>

<!ELEMENT DFRPackagesFile (Game | AutoSetup | Language | ExePackage | Icon | IconSet)+>
<!ATTLIST DFRPackagesFile
	Name CDATA #REQUIRED
	LastUpdateDate CDATA #REQUIRED
>

<!ELEMENT Game (#PCDATA)>
<!ATTLIST Game
	Name CDATA #REQUIRED
